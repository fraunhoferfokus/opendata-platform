<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>de.fhg.fokus.odp.portal.uploaddata-portlet</display-name>
  <servlet>
    <servlet-name>de.fhg.fokus.odp.portal.uploaddata</servlet-name>
    <servlet-class>de.fhg.fokus.odp.portal.uploaddata.servlet.UploadDataServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>de.fhg.fokus.odp.portal.uploaddata</servlet-name>
    <url-pattern>/myservlet/uploadServlet</url-pattern>
  </servlet-mapping>
  <jsp-config>
    <taglib>
      <taglib-uri>http://java.sun.com/portlet_2_0</taglib-uri>
      <taglib-location>
  			/WEB-INF/tld/liferay-portlet.tld
  		</taglib-location>
    </taglib>
  </jsp-config>
</web-app>