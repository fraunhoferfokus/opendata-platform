<?xml version="1.0"?>
<!DOCTYPE routes PUBLIC "-//Liferay//DTD Friendly URL Routes 6.0.0//EN" "http://www.liferay.com/dtd/liferay-friendly-url-routes_6_0_0.dtd">
<routes>
	<route>
		<pattern>/view/{packageId}</pattern>
		<implicit-parameter name="action">packageDetailsAction</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
	</route>
	<route>
		<pattern>/{searchQuery}</pattern>
		<implicit-parameter name="action">packageSearchAction</implicit-parameter>
		<implicit-parameter name="p_p_lifecycle">1</implicit-parameter>
	</route>
</routes>